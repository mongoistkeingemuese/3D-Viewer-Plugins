{
  "permissions": {
    "allow": [
      "Bash(npm run *)",
      "Bash(npm install)",
      "Bash(npm test)",
      "Bash(node --version)",
      "Bash(npm --version)",
      "Bash(ls *)",
      "Bash(cat *)",
      "Read(docs/**)",
      "Read(packages/**)",
      "Read(plugins/**)"
    ],
    "deny": [
      "Bash(rm -rf *)",
      "Bash(git push --force *)"
    ]
  },
  "env": {
    "NODE_ENV": "development"
  }
}
